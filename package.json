    "babel-cli": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.0.5",
    "markdown-it": "^8.4.1",
    "prismjs": "^1.14.0"
  },
  "peerDependencies": {
    "gatsby": "^1.0.0"
  }
}
