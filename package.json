{
  "name": "gatsby-plugin-mdx-prismjs",
  "version": "1.0.2",
  "description": "mdx plugin for gatsby with prismjs highlighting",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
